{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "readwisehighlights",
  "title": "ReadwiseHighlights",
  "description": "",
  "icon": "command-icon.png",
  "author": "markb",
  "license": "MIT",
  "commands": [
    {
      "name": "readwise",
      "title": "Readwise Highlights",
      "description": "Search your readwise highlights",
      "mode": "view"
    },
    {
      "name": "readwisebooks",
      "title": "Readwise Books",
      "description": "Search your readwise books",
      "mode": "view"
    }
  ],
  "preferences": [
    {
      "name": "token",
      "type": "password",
      "required": true,
      "title": "API Token",
      "description": "Your Readwise API access token",
      "placeholder": "Enter your API access token"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.30.3",
    "axios": "^0.26.1",
    "got": "^12.0.2",
    "node-fetch": "^3.2.3",
    "rss-parser": "^3.12.0"
  },
  "devDependencies": {
    "@types/node": "~16.10.0",
    "@types/react": "^17.0.28",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "react-devtools": "^4.21.0",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint"
  }
}
